<mvc:View displayBlock="true" controllerName="gubretas.mm.hay.controller.Master" height="100%" xmlns="sap.m" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc">
	<f:DynamicPage id="dynamicPageId" toggleHeaderOnTitleClick="false" fitContent="true" class="sapUiNoContentPadding">

		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title id="masterHeaderTitle" text="{
					parts: [
						'i18n>masterTitleCount',
						'application>/listCount'
					],
					formatter: 'jQuery.sap.formatMessage'
			}" />
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>
		<!-- text="{application>/listCount}"/> -->
		<f:content>
			<List class="sapUiNoMargin" items="{
				path: 'application>/DataModel/results'
			}">
				<headerToolbar>
					<OverflowToolbar>
						<SearchField search="onSearch" width="17.5rem" />
						<!-- <OverflowToolbarButton icon="sap-icon://sort" type="Transparent" press="onSort" /> 
					<Button text="Yeni Malzeme" icon="sap-icon://add" type="Transparent" press="onAdd" visible="{application>/UiModel/CreateActive}" />-->
					</OverflowToolbar>
				</headerToolbar>
				<ObjectListItem title="SAS : {application>EbelnPo}" intro="Sözleşme : {application>EbelnContract}" type="Active" number="30.000 TRY"  detailPress="onListItemPress" press="onListItemPress">
					<firstStatus>
						<ObjectStatus text="Tutarsal %120" state="Success" />
					</firstStatus>
					<secondStatus>
						<ObjectStatus text="Miktarsal %80" state="Error" />
					</secondStatus>
					<ObjectAttribute text="{application>Name1}" />
					<ObjectAttribute text="{application>Lifnr}" />

					<!-- <ObjectAttribute text="{Width} x {Depth} x {Height} {DimUnit}" /> -->
				</ObjectListItem>
			</List>
		</f:content>

		<!-- DynamicPage Footer -->
	</f:DynamicPage>
</mvc:View>