													<Label text="Fatura No" />
													<Input editable="{= ${application>UiConfig/BasisViewEditable} &amp;&amp; ${application>ProcessType} === '01' ? true : false }" value="{application>Mtart}" width="10rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" maxLength="{= parseInt(${mainModel>/#DataModel/Mtart/@maxLength}) }" required="true" />

													<Label text="Fatura Tutarı" />
