															</t:template>
														</t:Column>
														<t:Column width="9rem">
															<Label text="Sözleşme Para Birimi" />
															<t:template>
																<ObjectStatus text="{Status}" state="{
								path: 'Available',
								formatter: '.formatAvailableToObjectState'
							}"/>
															</t:template>
														</t:Column>
														<t:Column width="9rem">
															<Label text="Gerçekleşen Birim Fiyat" />
															<t:template>
																<u:Currency value="{Price}" currency="{CurrencyCode}"/>
															</t:template>
														</t:Column>
														<t:Column width="12rem">
															<Label text="Gerçekleşen İş Miktarı" />
															<t:template>
																<ComboBox value="{SupplierName}" items="{/Suppliers}">
																	<core:Item text="{Name}"/>
																</ComboBox>
															</t:template>
														</t:Column>
														<t:Column width="9rem">
															<Label text="Gerçekleşen İşin Birimi" />
															<t:template>
																<Link text="Show Image" href="{ProductPicUrl}" target="_blank"/>
															</t:template>
														</t:Column>
														<t:Column width="9rem">
															<Label text="Details" />
															<t:template>
																<Button text="Show Details" press="handleDetailsPress"/>
															</t:template>
														</t:Column>
														
													</t:columns>
												</t:Table>


