											<form:SimpleForm maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
												<form:content>

													<core:Title text="" />

													<Label text="Fatura Tarihi" />
