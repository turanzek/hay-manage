													<Label text="Personel No" />
													<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>VendorChoice}" width="15rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />

													<Label text="Personel adı soyadı" />
													<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>StorageText}" width="20rem" />

													<Label text="Operasyon tanımı" />
													<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>StorageText}" width="20rem" />


												</form:content>
											</form:SimpleForm>
