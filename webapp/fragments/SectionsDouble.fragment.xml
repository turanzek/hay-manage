<core:FragmentDefinition xmlns:uxap="sap.uxap" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table">

	<uxap:ObjectPageSection title="Malzeme verileri">
		<uxap:subSections>
			<uxap:ObjectPageSubSection title="Temel Veriler" visible="{application>UiConfig/BasisViewActive}">
				<uxap:blocks>
					<VBox>
						<HBox justifyContent="SpaceBetween">
							<form:SimpleForm maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" title="Mevcut veri">
								<form:content>

									<Label text="Malzeme tipi" />
									<Input editable="false" value="{application>ActiveValues/results/0/MaterialKind}" width="5rem" />

									<Label text="Malzeme türü" />
									<Input editable="false" value="{application>ActiveValues/results/0/Mtart}" width="10rem" maxLength="{= parseInt(${mainModel>/#DataModel/Mtart/@maxLength}) }" />

									<Label text="Mal grubu" />
									<Input editable="false" value="{application>ActiveValues/results/0/Matkl}" width="10rem" />

									<Label text="Malzeme kısa metni" />
									<Input editable="false" value="{application>ActiveValues/results/0/Maktx}" width="20rem" />

									<Label text="Temel ölçü brm." />
									<Input editable="false" value="{application>ActiveValues/results/0/Meins}" width="5rem" />


									<Label text="Harici mal gr." />
									<Input editable="false" value="{application>ActiveValues/results/0/Extwg}" width="10rem" />

									<Label text="Satıcı tercihi" />
									<Input editable="false" value="{application>ActiveValues/results/0/VendorChoice}" width="15rem" />

									<Label text="Depo yeri metni" />
									<Input editable="false" value="{application>ActiveValues/results/0/StorageText}" width="20rem" />

									<Label text="Malzeme notu" />
									<Input editable="false" value="{application>ActiveValues/results/0/MaterialNote}" width="20rem" />

								</form:content>
							</form:SimpleForm>
							<form:SimpleForm maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" title="Yeni veri">
								<form:content>

									<Label text="Malzeme tipi" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>MaterialKind}" width="5rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" required="true" />

									<Label text="Malzeme türü" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>Mtart}" width="10rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" maxLength="{= parseInt(${mainModel>/#DataModel/Mtart/@maxLength}) }" required="true" />

									<Label text="Mal grubu" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>Matkl}" width="10rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" required="true" />

									<Label text="Malzeme kısa metni" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>Maktx}" width="20rem" required="true" />

									<Label text="Temel ölçü brm." />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>Meins}" width="5rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" required="true" />

									<Label text="Harici mal gr." />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>Extwg}" width="10rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />

									<Label text="Satıcı tercihi" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>VendorChoice}" width="15rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />

									<Label text="Depo yeri metni" />
									<Input editable="{application>UiConfig/BasisViewEditable}" value="{application>StorageText}" width="20rem" />

									<Label text="Malzeme notu" />
									<Input  editable="{application>UiConfig/BasisViewEditable}" value="{application>MaterialNote}" width="20rem" />

								</form:content>
							</form:SimpleForm>

						</HBox>


						<HBox justifyContent="SpaceBetween" class="sapUiSmallMarginTop">

							<table:Table rows="{application>ActiveValues/results/0/SupplierCodes/results}" class="sapUiSmallMarginEnd" selectionMode="MultiToggle" visibleRowCount="5" ariaLabelledBy="title">
								<table:extension>
									<OverflowToolbar style="Clear">
										<Title text="Mevcut Tedarikçi Parça numaraları" />
										<ToolbarSpacer />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalemi Sil" press="onPressDeleteLine" icon="sap-icon://delete" />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalem ekle" press="onPressAddLine" icon="sap-icon://add" />
									</OverflowToolbar>
								</table:extension>
								<table:columns>
									<table:Column>
										<Label text="Satıcı numarası" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Lifnr}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />
										</table:template>
									</table:Column>
									<table:Column hAlign="End">
										<Label text="Tedarikçi parça no" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Idnlf}" />
										</table:template>
									</table:Column>
								</table:columns>

							</table:Table>

							<table:Table rows="{application>SupplierCodes/results}" class="sapUiSmallMarginEnd" selectionMode="MultiToggle" visibleRowCount="5" ariaLabelledBy="title">
								<table:extension>
									<OverflowToolbar style="Clear">
										<Title text="Yeni Tedarikçi Parça numaraları" />
										<ToolbarSpacer />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalemi Sil" press="onPressDeleteLine" icon="sap-icon://delete" />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalem ekle" press="onPressAddLine" icon="sap-icon://add" />
									</OverflowToolbar>
								</table:extension>
								<table:columns>
									<table:Column>
										<Label text="Satıcı numarası" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Lifnr}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />
										</table:template>
									</table:Column>
									<table:Column hAlign="End">
										<Label text="Tedarikçi parça no" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Idnlf}" />
										</table:template>
									</table:Column>
								</table:columns>

							</table:Table>
						</HBox>

						<HBox>
							<table:Table rows="{application>ActiveValues/results/0/Equipments/results}" selectionMode="MultiToggle" visibleRowCount="5" ariaLabelledBy="title">
								<table:extension>
									<OverflowToolbar style="Clear">
										<Title text="Mevcut Bağlı ekipmanlar" />
										<ToolbarSpacer />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalemi Sil" press="onPressDeleteLine" icon="sap-icon://delete" />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalem ekle" press="onPressAddLine" icon="sap-icon://add" />
									</OverflowToolbar>
								</table:extension>
								<table:columns>
									<table:Column hAlign="End">
										<Label text="Ekipman numarası" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Equnr}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />
										</table:template>
									</table:Column>
								</table:columns>
							</table:Table>


							<table:Table rows="{application>Equipments/results}" selectionMode="MultiToggle" visibleRowCount="5" ariaLabelledBy="title">
								<table:extension>
									<OverflowToolbar style="Clear">
										<Title text="Yeni Bağlı ekipmanlar" />
										<ToolbarSpacer />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalemi Sil" press="onPressDeleteLine" icon="sap-icon://delete" />
										<Button visible="{application>UiConfig/BasisViewEditable}" text="Kalem ekle" press="onPressAddLine" icon="sap-icon://add" />
									</OverflowToolbar>
								</table:extension>
								<table:columns>
									<table:Column hAlign="End">
										<Label text="Ekipman numarası" />
										<table:template>
											<Input editable="{detail>/UiConfig/BasisViewEditable}" value="{application>Equnr}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" />
										</table:template>
									</table:Column>
								</table:columns>
							</table:Table>
						</HBox>
					</VBox>

				</uxap:blocks>
			</uxap:ObjectPageSubSection>
			<uxap:ObjectPageSubSection title="MIP Verileri" visible="{application>UiConfig/MipViewActive}">
				<uxap:blocks>
					<HBox justifyContent="SpaceBetween">
						<form:SimpleForm editable="true" layout="GridLayout" labelSpanL="1" labelSpanM="1" columnsL="2" columnsM="2" title="Mevcut veri">
							<form:content>

								<core:Title text="" />

								<Label text="Üretim Yeri" />
								<Input editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Werks}" width="6rem" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Yen.sprş.svy" />
								<Input visible="{= ${application>MaterialKind} === 'ST' ? true : false}" editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Minbe}" width="6rem" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Asg.parti bykl." />
								<Input visible="{= ${application>MaterialKind} === 'ST' ? true : false}" editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Bstmi}" width="6rem" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Azm.parti bykl." />
								<Input visible="{= ${application>MaterialKind} === 'ST' ? true : false}" editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Bstma}" width="6rem" />

								<core:Title text="" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Azm.stok düzeyi" />
								<Input editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Mabst}" width="6rem" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Emniyet stoku" />
								<Input visible="{= ${application>MaterialKind} === 'ST' ? true : false}" editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Eisbe}" width="6rem" />

								<Label visible="{= ${application>MaterialKind} === 'ST' ? true : false}" text="Asg.emn.stoku" />
								<Input visible="{= ${application>MaterialKind} === 'ST' ? true : false}" editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Eislo}" width="6rem" />
							</form:content>
						</form:SimpleForm>

						<form:SimpleForm editable="true" layout="GridLayout" labelSpanL="1" labelSpanM="1" columnsL="2" columnsM="2" title="Yeni veri" labelMinWidth="300">
							<form:content>

								<core:Title text="" />

								<Label text="Üretim Yeri" />
								<Input editable="{=  ${application>UiConfig/MipViewEditable}  &amp;&amp; ${application>UiConfig/WerksEditable}  }" value="{application>Plants/results/0/Werks}" width="6rem" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" required="true" />

								<Label text="Yeniden sprş.svy" />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Minbe}" width="6rem" />

								<Label text="Asg. parti bykl." />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Bstmi}" width="6rem" />

								<Label text="Azm. parti bykl." />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Bstma}" width="6rem" />

								<core:Title text="" />

								<Label text="Azm. stok düzeyi" />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Mabst}" width="6rem" required="true" />

								<Label text="Emniyet stoku" />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Eisbe}" width="6rem" required="true" />

								<Label text="Asg. emn. stoku" />
								<Input type="Number" editable="{application>UiConfig/MipViewEditable}" value="{application>Plants/results/0/Eislo}" width="6rem" required="true" />
							</form:content>
						</form:SimpleForm>
					</HBox>
				</uxap:blocks>
			</uxap:ObjectPageSubSection>
			<uxap:ObjectPageSubSection title="Satınalma / Depo Verileri" visible="{application>UiConfig/PurchaseViewActive}">
				<uxap:blocks>
					<HBox justifyContent="SpaceBetween">
						<form:SimpleForm maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" title="Mevcut veri">
							<form:content>

								<Label text="Depo yeri" />
								<Input editable="false" value="{application>Storages/results/0/Lgort}" width="15rem" />

								<Label text="MİP krkt." />
								<Input editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Dismm}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" width="15rem" />

								<Label text="Dış.tdrk.DY" />
								<Input editable="false" value="{application>ActiveValues/results/0/Plants/results/0/Lgfsb}" width="15rem" />

								<Label text="Depo adresi" />
								<Input editable="false" value="{application>ActiveValues/results/0/Storages/results/0/Lgpbe}" width="15rem" />

								<Label text="Standart fiyat" />
								<Input editable="false" value="{application>ActiveValues/results/0/Valuations/results/0/Stprs}" width="15rem" />

								<Label text="Değerleme snf." />
								<Input editable="false" value="{application>ActiveValues/results/0/Valuations/results/0/Bklas}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" width="15rem" />

							</form:content>
						</form:SimpleForm>
						<form:SimpleForm maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1" title="Yeni veri">
							<form:content>

								<Label text="Depo yeri" />
								<Input editable="false" value="{application>Storages/results/0/Lgort}" width="15rem" />

								<Label text="MİP krkt." />
								<Input editable="{application>UiConfig/PurchaseViewEditable}" value="{application>Plants/results/0/Dismm}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" width="15rem" />

								<Label text="Dış.tdrk.DY" />
								<Input editable="{application>UiConfig/PurchaseViewEditable}" value="{application>Plants/results/0/Lgfsb}" width="15rem" />

								<Label text="Depo adresi" />
								<Input editable="{application>UiConfig/PurchaseViewEditable}" value="{application>Storages/results/0/Lgpbe}" width="15rem" />

								<Label text="Standart fiyat" />
								<Input editable="{application>UiConfig/PurchaseViewEditable}" value="{application>Valuations/results/0/Stprs}" width="15rem" />

								<Label text="Değerleme snf." />
								<Input editable="{application>UiConfig/PurchaseViewEditable}" value="{application>Valuations/results/0/Bklas}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested" width="15rem" />

							</form:content>
						</form:SimpleForm>
					</HBox>
				</uxap:blocks>
			</uxap:ObjectPageSubSection>
		</uxap:subSections>
	</uxap:ObjectPageSection>

	<core:Fragment fragmentName="gubretas.mm.hay.fragments.OtherSections" type="XML" />


</core:FragmentDefinition>